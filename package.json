{
  "name": "ai-test-automation",
  "version": "1.0.0",
  "description": "Automated test case generation and BrowserStack integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "test": "npm run test:e2e",
    "test:e2e": "tsx tests/test-e2e.ts",
    "test:resolvers": "tsx tests/test-resolvers.ts",
    "test:all": "npm run test:resolvers && npm run test:e2e"
  },
  "keywords": ["browserstack", "jira", "automation", "testing", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
